FOREACH(ignoreMe IN CASE WHEN event.op='c' THEN [1] ELSE [] END | MERGE (t:Testing_ifg2 {id : event.after.id}) on create set t.value = event.after.value) FOREACH(ignoreMe IN CASE WHEN event.op='d' THEN [1] ELSE [] END | MERGE (t:Testing_ifg2 {id : event.before.id}) detach delete t) FOREACH(ignoreMe IN CASE WHEN event.op='u' THEN [1] ELSE [] END | MERGE (t:Testing_ifg2 {id : event.after.id}) SET t.value = event.after.value)
